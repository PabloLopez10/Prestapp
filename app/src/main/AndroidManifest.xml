<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pablolopez.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registro"
            android:parentActivityName="com.pablolopez.myapp.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pablolopez.myapp.MainActivity" />
        </activity>
        <activity
            android:name=".Inicio"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity android:name=".Notificacion"
            android:parentActivityName="com.pablolopez.myapp.Inicio">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pablolopez.myapp.Inicio" />
        </activity>
        <activity android:name=".NuevoPrestamo"
            android:parentActivityName="com.pablolopez.myapp.Inicio">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pablolopez.myapp.Inicio" />
        </activity>
        <activity android:name=".Verificar"></activity>
    </application>

</manifest>